{% do asset('js', [
    'local:/elfinder/js/elfinder.min.js',
    'modules/elfinder/extensions/tinymce5/tinymce5ElfinderExt.js',
]) %}
{% do asset('css', [
    '//cdnjs.cloudflare.com/ajax/libs/jqueryui/1.12.1/themes/smoothness/jquery-ui.css',
    'local:/elfinder/css/elfinder.min.css',
    'local:/elfinder/css/theme.css',
]) %}


<script>
        const mceElf = new tinymceElfinder({
            // connector URL (Use elFinder Demo site's connector for this demo)
            url: '{{ path('@elfinder_connector') }}',
            // upload target folder hash for this tinyMCE
            uploadTargetHash: 'l1_Lw',
            // elFinder dialog node id
            nodeId: 'elfinder'
        });
</script>
